                "clickUpApi": {
                    "id": "clickup_main"
                }
            }
        },
        {
            "parameters": {
                "conditions": {
                    "number": [
                        {
                            "value1": "={{$json.length}}",
                            "operation": "equal",
                            "value2": 0
                        }
                    ]
                }
            },
            "name": "No Duplicate?",
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                700,
                200
            ]
        },
        {
